  <DataSources>
    <DataSource Name="NDM">
      <DataSourceReference>NDM</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>34d04abb-0711-44de-997b-9a42901ae1c0</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NDM</DataSourceName>
        <CommandText>SELECT        TOP (15) t.ConsumerId, CASE WHEN (len(r.SPAcctNo) &lt; 1 OR
                         r.SPAcctNo IS NULL) THEN am.SPAcctNo ELSE r.SPAcctNo END AS SPAcctNo, r.RawAddrSource, GETDATE() AS DATE
FROM            TLORESPONSE AS t INNER JOIN
                         RAWADDRESS AS r ON t.ConsumerId = r.ConsumerId AND r.RawAddrSource = 'TLO' INNER JOIN
                         ADDRESS AS a ON t.ConsumerId = a.ConsumerId INNER JOIN
                         CONSUMERACCOUNTLINK AS c ON t.ConsumerId = c.ConsumerId INNER JOIN
                         CONSUMERMATRIX AS cm ON t.ConsumerId = cm.ConsumerId AND cm.SPSeqNo = 1 INNER JOIN
                         ACCOUNTMATRIX AS am ON c.AccountId = am.AccountId
WHERE        (t.UpdatedOnDTTM &lt; GETDATE() - 7)
ORDER BY NEWID()</CommandText>
      </Query>
      <Fields>
        <Field Name="ConsumerId">
          <DataField>ConsumerId</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="SPAcctNo">
          <DataField>SPAcctNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RawAddrSource">
          <DataField>RawAddrSource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DATE">
          <DataField>DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
