  <DataSources>
    <DataSource Name="NDM">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=agl_ndm_dev1;Initial Catalog=Apollo_NDM</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>77593823-f91d-4c00-8bc4-663fb3278d34</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NDM</DataSourceName>
        <CommandText>SELECT TOP 15 
   c.[ConsumerId]
   , CASE
      WHEN (len(r.SPAcctNo)&lt;1 OR r.SPAcctNo is null)
         THEN am.SPAcctNo
         ELSE r.SPAcctNo
     END as SPAcctNo
   , r.RawAddrSource
   , [DATE] = GETDATE()
   --, r.RawAddrCreatedDTTM
   --, a.AddrScoreDTTM
   --, c.UpdatedOnDTTM AS CBCRESPUpdatedOnDTTM
FROM [APOLLO_NDM].[dbo].[CBCRESPONSE] as c
   INNER JOIN [APOLLO_NDM].[dbo].[RAWADDRESS] as r
      ON c.ConsumerId = r.ConsumerId
      AND RawAddrSource = 'CBC Innovis'
   INNER JOIN [APOLLO_NDM].[dbo].[ADDRESS] as a
      ON c.ConsumerId = a.ConsumerId
   INNER JOIN [APOLLO_NDM].[dbo].[CONSUMERACCOUNTLINK] as ca
      ON c.ConsumerId = ca.ConsumerId
   INNER JOIN [APOLLO_NDM].[dbo].[CONSUMERMATRIX] as cm
      ON c.ConsumerId = cm.ConsumerId
      AND cm.SPSeqNo = 1
   INNER JOIN [APOLLO_NDM].[dbo].[ACCOUNTMATRIX] as am
      ON ca.AccountId = am.AccountId
WHERE c.UpdatedOnDTTM &lt; (GETDATE() - 7)
ORDER BY NEWID()</CommandText>
      </Query>
      <Fields>
        <Field Name="ConsumerId">
          <DataField>ConsumerId</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="SPAcctNo">
          <DataField>SPAcctNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RawAddrSource">
          <DataField>RawAddrSource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DATE">
          <DataField>DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
